
@{
    ViewBag.Title = "ViewComplainByProgress";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Complaint By Progress</h2>
<div class="form-inline">
    <div class="form-group">
        <label>Progress :</label>
        @Html.DropDownList("progressFrom", null, htmlAttributes: new { @class = "form-control" })
        @Html.DropDownList("progressTo", null, htmlAttributes: new { @class = "form-control" })
        <button class="btn btn-primary" id="btPrint">
            <span>Show</span>
        </button>
    </div>
</div>
